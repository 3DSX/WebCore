@*
    @model List<ParentEntityModel>
*@
<a class="btn btn-info" asp-action="Create">Crear</a>
<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.UniqueId</td>
                <td>@obj.Nombre</td>
                <td><a class="btn btn-light" asp-action="Index" asp-route-id="@obj.UniqueId">Ver</a></td>
                <td><a class="btn btn-primary" asp-action="Edit" asp-route-id="@obj.UniqueId">Editar</a></td>
                <td><a class="btn btn-danger" asp-action="Delete" asp-route-id="@obj.UniqueId">Eliminar</a></td>
            </tr>
        }
    </tbody>
</table>